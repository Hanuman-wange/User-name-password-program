	public static void main(String[] args) {
		// TODO Auto-generated method stub
     String username = JOptionPane.showInputDialog("Username:");
     String password = JOptionPane.showInputDialog("Password");
     
  if (username != null && password != null && username.equals("Hanuman") && password.equals("123")||
		  username.equals("Ramu")&&password.equals("567")) {
	  JOptionPane.showMessageDialog(null, "you are in.");
  }else {
	  JOptionPane.showMessageDialog(null, "you are suspicious.");
	  
  }
	  
	
  
	}

}
